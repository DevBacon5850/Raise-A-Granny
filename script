    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(54.1543617, 6.24803162, -22.1094532, 0.999758244, 3.94105335e-08, 0.0219867341, -3.8176367e-08, 1, -5.65521354e-08, -0.0219867341, 5.56990898e-08, 0.999758244)
