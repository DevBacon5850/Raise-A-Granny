local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Bacon Hub", "DarkTheme")
--- Tabs & Sections
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Main")
Section:NewButton("Inf Money", "Click To Get Inf Money", function()
    while task.wait() do
        local event = game:GetService("ReplicatedStorage").cleanPoop
        event:FireServer()
        local event = game:GetService("ReplicatedStorage").SaveGame
        event:FireServer()
        end
end)
local Section = Tab:NewSection("Teleports")
Section:NewButton("Main Room", "Click To Teleport To Main Room", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1.3802079, 5.24802399, 1.3329916, 5.01330987e-06, -4.55473259e-09, 1, 6.58296265e-11, 1, 4.55473215e-09, -1, 6.58067975e-11, 5.01330987e-06)
end)
Section:NewButton("Second Room", "Click To Teleport To Second Room", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(37.0764999, 5.24802494, -16.0417118, -0.018854253, -2.56105959e-09, -0.999822259, 1.83111515e-09, 1, -2.59604538e-09, 0.999822259, -1.87973614e-09, -0.018854253)
end)
Section:NewButton("Plant Room", "Click To Teleport To Plant Room", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(53.2119713, 5.24802399, 9.16897774, 0.999920964, -6.98686833e-08, -0.0125703262, 7.1024175e-08, 1, 9.14760037e-08, 0.0125703262, -9.23615744e-08, 0.999920964)
end)
Section:NewButton("Upstair Room", "Click To Teleport To Upstair Room", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0.715339959, 21.4993019, -0.144258752, 0.0251349565, 2.37334818e-10, 0.999684095, -2.70037881e-10, 1, -2.30620301e-10, -0.999684095, -2.64155919e-10, 0.0251349565)
end)
